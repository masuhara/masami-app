<h3>長澤まさみさんへメッセージを送る</h3>
<ul class="list-group">
  <% @projects.each do |project| %>
  <li class="list-group-item">
    <%= link_to project.title, project_path(project.id) %>
    <%= link_to "編集", edit_project_path(project.id), class: "btn btn-default" %>
    <%= link_to "削除", project_path(project.id), method: :delete, data: {
      confirm: "本当に削除しますか?" }, class: "btn btn-default" %>
  </li>
  <% end %>
</ul>

<p><%= link_to "投稿する", new_project_path, class: "btn btn-default" %></p>
